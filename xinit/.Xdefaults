URxvt.depth:                32
URxvt.geometry:             90x30
URxvt.transparent:          true
URxvt.shading:              40
URxvt.visualBell:           false
URxvt.loginShell:           true
URxvt.saveLines:            2048
URxvt.internalBorder:       3
URxvt.lineSpace:            0
URxvt.allow_bold:           true
URxvt.font:                 xft:DejaVuSansMono:size=8:antialias=false
URxvt*letterSpace:          0
URxvt.scrollStyle:          rxvt
URxvt.scrollBar:            false
URxvt.cursorBlink:          true
URxvt.cursorColor:          #77bb33
URxvt.cursorUnderline:      false
! special
URxvt*.foreground:   #d0d0d0
URxvt*.background:   #151515
URxvt*.cursorColor:  #d0d0d0
! black
URxvt*.color0:       #151515
URxvt*.color8:       #505050
! red
URxvt*.color1:       #ac4142
URxvt*.color9:       #ac4142
! green
URxvt*.color2:       #90a959
URxvt*.color10:      #90a959
! yellow
URxvt*.color3:       #f4bf75
URxvt*.color11:      #f4bf75
! blue
URxvt*.color4:       #4b7180
URxvt*.color12:      #4b7180
! magenta
URxvt*.color5:       #aa759f
URxvt*.color13:      #aa759f
! cyan
URxvt*.color6:       #75b5aa
URxvt*.color14:      #75b5aa
! white
URxvt*.color7:       #d0d0d0
URxvt*.color15:      #f5f5f5
!! Copy/paste
URxvt.keysym.Shift-Control-V:    perl:clipboard:paste
URxvt.iso14755:                  False
URxvt.perl-ext-common:           default,clipboard,matcher
!! Extensions and URLs
URxvt.url-launcher:              /usr/bin/firefox
URxvt.matcher.button:            1
